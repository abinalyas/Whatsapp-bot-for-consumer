{
  "version": 2,
  "buildCommand": "echo 'FORCE DEPLOYMENT - v2.14.3 - $(date) - $(uuidgen)'",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/public/**",
          "vercel-test.txt"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}