{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.vercel.js"
    },
    {
      "src": "/webhook",
      "dest": "dist/index.vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.vercel.js"
    }
  ]
}